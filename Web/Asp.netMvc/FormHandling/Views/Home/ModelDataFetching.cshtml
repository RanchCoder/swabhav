@model FormHandling.Models.User

@{
    ViewBag.Title = "ModelDataFetching";
}

<h2>Fetching user data using model</h2>

<div>
    @using (Html.BeginForm("DynamicTextBox", "Home", FormMethod.Get))
    {

        @Html.TextBoxFor(m => m.Id)

        <br />
        <br />
        @Html.TextBoxFor(m => m.Name)


        <br />
        <br />
        <div>
            <input type="Submit" value="submit" />
        </div>
    }

</div>




